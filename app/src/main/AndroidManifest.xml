<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mediaplayer2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RockActivity"
            android:label="@string/rockText" />
        <activity
            android:name=".ClassicActivity"
            android:label="@string/classicText" />
        <activity
            android:name=".JazzActivity"
            android:label="@string/jazzText" />
        <activity
            android:name=".ElectronicActivity"
            android:label="@string/electronicText" />
        <activity
            android:name=".RockSongActivity"
            android:label="@string/rockSongText"/>

    </application>
</manifest>